{
  "name": "flem",
  "version": "1.0.0",
  "description": "Flem is a local emulator for the App Engine Flexible Environment.",
  "bin": {
    "flem": "./bin/cli.js"
  },
  "scripts": {
    "start": "npm run build:live",
    "build:live": "tsc --watch",
    "build": "tsc",
    "postinstall": "typings install",
    "test": "mocha",
    "example": "node ./bin/cli.js -p 8080 ~/Code/express-test/"
  },
  "keywords": [
    "App Engine",
    "Emulator",
    "dev_appserver"
  ],
  "author": "Justin Beckwith<justin.beckwith@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "chokidar": "^1.6.1",
    "commander": "^2.9.0",
    "fs-extra": "^1.0.0",
    "js-yaml": "^3.7.0",
    "lodash": "^4.17.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "request": "^2.79.0",
    "typescript": "^2.1.5",
    "typings": "^2.1.0"
  }
}
