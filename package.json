{
  "name": "flem",
  "version": "1.0.11",
  "description": "Flem is a local emulator for the App Engine Flexible Environment.",
  "bin": {
    "flem": "./bin/cli.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run compile:live",
    "build:live": "tsc --watch",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "compile": "tsc -p .",
    "test": "mocha --require source-map-support/register build/test",
    "fix": "gts fix",
    "check": "gts check",
    "clean": "gts clean",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustinBeckwith/flem.git"
  },
  "bugs": {
    "url": "https://github.com/JustinBeckwith/flem/issues"
  },
  "keywords": [
    "App Engine",
    "Emulator",
    "dev_appserver"
  ],
  "author": "Justin Beckwith<justin.beckwith@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "commander": "^2.14.0",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.11",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.5",
    "uuid": "^3.2.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.0",
    "@types/handlebars": "^4.0.36",
    "@types/js-yaml": "^3.10.1",
    "@types/lodash": "^4.14.101",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "@types/request": "^2.47.0",
    "@types/source-map-support": "^0.4.0",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "google-ts-style": "^0.2.0",
    "gts": "^0.5.3",
    "mocha": "^5.0.0",
    "request": "^2.83.0",
    "source-map-support": "^0.5.3",
    "typescript": "^2.7.1"
  }
}
